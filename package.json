{
  "name": "generator-git",
  "description": "Create the configurable scaffolding to start with a git-like project.",
  "homepage": "https://github.com/Kikobeats/generator-git",
  "version": "3.1.0",
  "main": "app/index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": "https://github.com/Kikobeats/generator-git",
  "bugs": {
    "url": "https://github.com/Kikobeats/generator-git/issues"
  },
  "keywords": [
    "generator",
    "git",
    "github",
    "initialize",
    "project",
    "scaffold",
    "yeoman",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "latest",
    "finepack": "latest",
    "github": "latest",
    "inquirer-npm-name": "latest",
    "lodash": "latest",
    "mkdirp": "latest",
    "superb": "latest",
    "yeoman-generator": "latest",
    "yosay": "latest"
  },
  "devDependencies": {
    "git-dirty": "latest",
    "standard": "latest"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "app"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "commit": "git add CHANGELOG.md package.json && git commit -m \"$newVersion releases\"",
