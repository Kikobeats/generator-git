{
  "name": "<%= appName %>",
  "description": "<%= appDescription %>",
  "homepage": "https://github.com/<%= userLogin %>/<%= appName %>",
  "version": "0.0.0",
  "main": "./index.js",
  "author": {
    "email": "<%= userEmail %>",
    "name": "<%= userName %>",
    "url": "<%= userUrl %>"
  },
  "repository": "<%= userLogin %>/<%= appName %>",
  "bugs": {
    "url": "https://github.com/<%= userLogin %>/<%= appName %>/issues"
  },
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "mocha": "latest",
    "should": "latest",
    "standard": "latest"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.4.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "dirty": "./node_modules/.bin/git-dirty",
    "commit": "git add CHANGELOG.md bower.json package.json dist && git commit -m \"$newVersion releases\"",
    "lint": "./node_modules/.bin/standard lib",
    "prepublish": "npm run dirty && npm run clean && npm install && npm test",
    "pretest": "npm run lint",
    "push": "git tag $newVersion && git push origin $newVersion --follow-tags",
    "test": " && sh test/test.sh"
  },
  "license": "MIT"
}
