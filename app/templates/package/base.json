{
  "name": "<%= appName %>",
  "description": "<%= appDescription %>",
  "homepage": "https://nicedoc.io/<%= userLogin %>/<%= appName %>",
  "version": "0.0.0",
  "main": "index.js",
  "author": {
    "email": "<%= userEmail %>",
    "name": "<%= userName %>",
    "url": "<%= userUrl %>"
  },
  "repository": "<%= userLogin %>/<%= appName %>",
  "bugs": {
    "url": "https://github.com/<%= userLogin %>/<%= appName %>/issues"
  },
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "",
    "postrelease": "npm run release:tags && npm run release:github && npm publish",
    "prerelease": "git-dirty && npm run update:check && npm test && npm run contributors",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "pretest": "npm run lint",
    "release": "git-authors-cli finepack && git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin master",
    "test": "nyc ",
    "update": "ncu -a",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ]
  }
}
